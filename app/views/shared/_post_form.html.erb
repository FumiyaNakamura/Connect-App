<%= form_for(@post) do |f| %>
    <% render "shared/error_messages", object: f.object %>
    <div>
        <%= f.label :タイトル  %><br>
        <%= f.text_area :title  %><br>
    </div>
    <div>
        <%= f.label :内容  %><br>
        <%= f.text_area :content  %><br>
    </div>
    <%= f.submit "投稿する", class: "btn btn-default" %>
<% end %>
    